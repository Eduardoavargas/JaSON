{
	"name": "JaSON",
	"version": "3.0.1",
	"manifest_version": 2,
	"description": "JaSON: JSON and XML web testing tool.",
	"app": {
		"launch": {
			"local_path": "JaSON.html"
		}
	},
	"icons": {
		"16": "images/icon-16x16.png",
		"48": "images/icon-48x48.png",
		"128": "images/icon-128x128.png"
	},
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"permissions": [
		"http://*/",
		"https://*/"
	]
}