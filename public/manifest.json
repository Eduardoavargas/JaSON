{
  "name": "JaSON",
  "version": "5.0.0",
  "manifest_version": 2,
  "description": "JaSON: JSON and XML web testing tool.",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "JaSON"
  },
  "icons": {
    "16": "icon-16x16.png",
    "48": "icon-48x48.png",
    "96": "icon-96x96.png",
    "128": "icon-128x128.png",
    "256": "icon-256x256.png"
  },
  "permissions": [
    "http://*/",
    "https://*/"
  ]
}